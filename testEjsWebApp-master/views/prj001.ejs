                                                                                <span class="" style="color: #204d74">
                                                                                    未审核
                                                                                </span>
                                                                            </a>
                                                                            </li>
                                                                </ul>
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <% }) %>
                                            </tbody>
                                        </table>
                                        <!-- 信息列表框 -->
                                        
                                        <!-- 分页框 -->
                                        <div class="row">
                                                <div class="col-sm-6">共有 <%= totalCount %> 条</div>

                                                <div id="pagination" class="col-sm-6 page"  style="float:right">
                                                    <script>
                                                    // 调用父页面的方法改变菜单指示
                                                    $(function(){
                                                        $("#pagination").pagination({
                                                            currentPage: parseInt(curpage_global),
                                                            totalPage: parseInt(totalpagenumber_global),
                                                            callback: function(current) {
                                                                if (($("#keyword").val()) == "") {
                                                                    window.location.href = "/prj001/?page=" + current;
                                                                } else {
                                                                    window.location.href = "/prj001/?page=" + current  + "&keyword=" + $("#keyword").val();
                                                                };
                                                            }
                                                        });
                                                    });
                                                    </script>
                                                </div>
                                        </div>
                                        <!-- 分页框 -->
                                    </div>
                                </div>
                            </div>

